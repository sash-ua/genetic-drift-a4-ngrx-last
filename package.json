{
  "name": "GeneticDrift-NGRX",
  "version": "2.0.0",
  "scripts": {
    "start": "gulp",
    "prod": "gulp prod",
    "copy:aot": "gulp copyDist:aot",
    "ngc:aot": "ngc -p tsconfig-aot.json",
    "rollup": "rollup -c rollup-config.js",
    "map": "source-map-explorer aot/dist/build.js",
    "1.test": "karma start karma.conf.js",
    "2.test": "remap-istanbul -i reports/coverage.json -o reports/coverage -t html && npm run test:launch-webserver",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "protractor": "node_modules/.bin/protractor protractor.conf.js",
    "test:server": "http-server -p 3000 -c-1 --cors",
    "test:e2e": "npm-run-all -s webdriver-update -p -r test:server webdriver-start protractor",
    "test:ci:karma": "karma start karma.conf.js --singleRun",
    "test:ci:all": "npm-run-all -s test:ci:karma webdriver-update -p -r test:server webdriver-start protractor",
    "test:launch-webserver": "gulp webserver:test",
    "lite": "lite-server -o",
    "lite:aot": "lite-server -c aot/bs-config.json",
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "tsc:aot": "tsc -p tsconfig-aot.json",
    "es5": "tsc --target es5 --allowJs aot/dist/build.js --out aot/dist/bundles.js"
  },
  "licenses": [
    {
      "type": "MIT"
    }
  ],
  "dependencies": {
    "@angular/animations": "^4.1.0-beta.1",
    "@angular/common": "^4.1.0-beta.1",
    "@angular/compiler": "^4.1.0-beta.1",
    "@angular/compiler-cli": "^4.1.0-beta.1",
    "@angular/core": "^4.1.0-beta.1",
    "@angular/forms": "^4.1.0-beta.1",
    "@angular/http": "^4.1.0-beta.1",
    "@angular/material": "2.0.0-beta.3",
    "@angular/platform-browser": "^4.1.0-beta.1",
    "@angular/platform-browser-dynamic": "^4.1.0-beta.1",
    "@angular/platform-server": "^4.1.0-beta.1",
    "@angular/router": "^4.1.0-beta.1",
    "@angular/service-worker": "^1.0.0-beta.8",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.2",
    "@ngrx/router-store": "^1.2.5",
    "@ngrx/store": "^2.2.1",
    "core-js": "^2.4.1",
    "intl": "^1.2.5",
    "ngrx-store-freeze": "^0.1.9",
    "reselect": "^3.0.0",
    "rxjs": "5.3.0",
    "systemjs": "^0.20.12",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@ngrx/store-devtools": "^3.2.4",
    "@ngrx/store-log-monitor": "^3.0.2",
    "@types/async": "^2.0.40",
    "@types/browser-sync": "^0.0.34",
    "@types/core-js": "^0.9.41",
    "@types/d3": "^4.7.0",
    "@types/gulp": "^4.0.2",
    "@types/gulp-sass": "^0.0.30",
    "@types/gulp-util": "^3.0.31",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "2.5.47",
    "@types/node": "^7.0.13",
    "@types/rimraf": "0.0.28",
    "@types/systemjs": "^0.20.2",
    "@types/zone.js": "^0.5.12",
    "async": "^2.3.0",
    "browser-sync": "^2.18.8",
    "d3": "^4.8.0",
    "es6-shim": "^0.35.3",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-clean-css": "^3.0.4",
    "gulp-gzip": "^1.4.0",
    "gulp-rimraf": "^0.2.1",
    "gulp-sass": "^3.1.0",
    "gulp-svg-sprite": "^1.3.6",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^4.3.11",
    "hammer-timejs": "^1.1.0",
    "hammerjs": "^2.0.8",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.5.2",
    "jasmine-spec-reporter": "^4.0.0",
    "karma": "^1.6.0",
