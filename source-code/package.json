    "lite": "lite-server -o",
    "serve": "http-server -p 9091 -c-1 -o",
    "lite:aot": "lite-server -c aot/bs-config.json",
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "tsc:aot": "tsc -p tsconfig-aot.json",
    "es5": "tsc --target es5 --allowJs aot/dist/build.js --out aot/dist/bundles.js"
  },
