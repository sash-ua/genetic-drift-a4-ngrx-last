{
  "name": "GeneticDrift-NGRX",
  "version": "1.0.0",
  "scripts": {
    "start": "gulp",
    "prod": "gulp prod",
    "copy:aot": "gulp copyDist:aot",
    "ngc:aot": "ngc -p tsconfig-aot.json",
    "rollup": "rollup -c rollup-config.js",
    "map": "source-map-explorer aot/dist/build.js",
    "1.test": "karma start karma.conf.js",
    "2.test": "remap-istanbul -i reports/coverage.json -o reports/coverage -t html && npm run test:launch-webserver",
